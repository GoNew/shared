<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.mapper.chatBoard">
	<select id="selectChatMsgToChatId" parameterType="Integer"
		resultType="ChatBoard">
		select		*
		from		CHAT_BOARD
		where		chat_id>=#{chatId}
		
	</select>
</mapper>
